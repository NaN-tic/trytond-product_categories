<data>
    <xpath expr="/form/notebook" position="replace_attributes">
		<page id="accounting" states="{'invisible': ~Eval('accounting')}"/>
	</xpath>

    <xpath expr="/form/field[@name='parent']" position="after">
        <label name="kind"/>
        <field name="kind"/>
        <label name="accounting"/>
        <field name="accounting"/>
        <label name="required"/>
        <field name="required"/>
        <label name="unique"/>
        <field name="unique"/>
        <label name="def_category"/>
        <field name="def_category"/>
        <newline/>
    </xpath>
</data>
